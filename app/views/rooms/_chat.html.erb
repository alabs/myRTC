<!-- chat window -->
<div id='chat-window'>
  <div class='column-pad'>
    <div id='chat-messages'>
      <div class='column-pad'>
        Chat
        <div class='chat-msg' id="chat-output">
        </div>
      </div>
    </div>
    <div id='chat-submission'>
      <div class="input-append">
        <input class="col-md-12" id="chat-input" type="text" autofocus="true" style="padding:0.2em; font-size:0.7em; width: 100%;" placeholder="Share something in the chat">
      </div>
    </div>
  </div>
</div>
